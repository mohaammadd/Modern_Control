        "# Define tank model function\n",
        "def tank_model(t, x):\n",
        "    A = 1.0\n",
        "    C = 2.0\n",
        "    F_in = 0\n",
        "    u = 0.1\n",
        "\n",
        "    xp = 1/A * (F_in - C * u * np.sqrt(x))\n",
        "    return xp"
      ]
